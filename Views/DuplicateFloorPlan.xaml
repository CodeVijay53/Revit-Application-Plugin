<Window x:Class="FinalApplication.Views.DuplicateFloorPlan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Duplicate Floor Plan" Height="300" Width="400" ResizeMode="NoResize">
    <Grid Margin="10,10,10,11">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBlock Text="Select a Floor Plan:" Margin="0,0,0,10" />
        <ComboBox x:Name="FloorPlanComboBox" Grid.Row="1" Margin="0,0,0,164" />
        <TextBlock Text="Enter Suffix:" Grid.Row="1" Margin="0,73,0,110" />
        <TextBox x:Name="SuffixTextBox" Grid.Row="1" Margin="0,89,0,89" />
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,170,0,10">
            <Button x:Name="OkButton" Content="OK" Width="80" Margin="5" Height="25" />
            <Button x:Name="CancelButton" Content="Cancel" Width="80" Margin="5" Height="25" />
        </StackPanel>
    </Grid>
</Window>
